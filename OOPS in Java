Object And Class...


class OOPS{
    public static void main(String[] args) {
        Pen P1= new Pen(); //Object are created..
        P1.setColor("red");
        P1.setTip(6);

        System.out.println(P1.color);
        System.out.println(P1.tip);

    }
}
class Pen{ // class and class name is pen..
    int tip;
    String color;
    void setColor(String newColor){
        color=newColor;
    }
    void setTip(int newTip){
        tip=newTip;
    }
}
